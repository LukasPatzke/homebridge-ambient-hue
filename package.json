{
  "displayName": "Homebridge Ambient Hue",
  "name": "homebridge-ambient-hue",
  "version": "0.0.3-beta.2",
  "description": "Dynamically adjusts your Philips Hue lights throughout the day.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/LukasPatzke/homebridge-ambient-hue.git"
  },
  "bugs": {
    "url": "https://github.com/LukasPatzke/homebridge-ambient-hue/issues"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.3.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "build": "npm run build --prefix ui && rimraf ./dist && tsc -p tsconfig.build.json",
    "postbuild": "npm run copy-migrations",
    "prepublishOnly": "npm run lint && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "watch:server": "nest start --watch",
    "watch": "nodemon",
    "start:server": "nest start",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -o -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "copy-migrations": "copyfiles -u 1 src/migration/*.js dist"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/platform-fastify": "^7.6.15",
    "@nestjs/platform-socket.io": "^7.6.15",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/terminus": "^7.1.1",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.15",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "fastify": "^3.14.1",
    "fastify-swagger": "^4.4.2",
    "fs-extra": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "socket.io-client": "2.4.0",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "homebridge": "^1.3.1",
    "jest": "26.6.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
