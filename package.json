{
  "displayName": "Homebridge Ambient Hue",
  "name": "homebridge-ambient-hue",
  "version": "0.0.4",
  "description": "Dynamically adjusts your Philips Hue lights throughout the day.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/LukasPatzke/homebridge-ambient-hue.git"
  },
  "bugs": {
    "url": "https://github.com/LukasPatzke/homebridge-ambient-hue/issues"
  },
  "engines": {
    "node": ">=14.0.0",
    "homebridge": ">=1.3.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "build": "npm run build --prefix ui && rimraf ./dist && tsc -p tsconfig.build.json",
    "postbuild": "npm run copy-migrations",
    "prepublishOnly": "npm run lint && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "watch:server": "nest start --watch",
    "watch": "nodemon",
    "start:server": "nest start",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -o -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "copy-migrations": "copyfiles -u 1 src/migration/*.js dist"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "dependencies": {
    "@nestjs/axios": "0.0.5",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.7",
    "@nestjs/core": "^8.2.6",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-fastify": "^8.2.6",
    "@nestjs/platform-socket.io": "^8.2.6",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.4",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.2.6",
    "axios": "^0.21.4",
    "chalk": "^4.1.0",
    "fastify": "^3.14.1",
    "fastify-swagger": "^4.4.2",
    "fs-extra": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "socket.io-client": "^4.4.1",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.6",
    "@nestjs/testing": "^8.2.6",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.6.1",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "homebridge": "^1.3.1",
    "jest": "26.6.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
