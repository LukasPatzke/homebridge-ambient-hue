{
  "displayName": "Homebridge Ambient Hue",
  "name": "homebridge-ambient-hue",
  "version": "0.0.7",
  "description": "Dynamically adjusts your Philips Hue lights throughout the day.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/LukasPatzke/homebridge-ambient-hue.git"
  },
  "bugs": {
    "url": "https://github.com/LukasPatzke/homebridge-ambient-hue/issues"
  },
  "engines": {
    "node": ">=14.0.0",
    "homebridge": ">=1.5.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "build": "npm run build --prefix ui && rimraf ./dist && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run lint && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "watch:server": "nest start --watch",
    "watch": "nodemon",
    "start:server": "nest start",
    "typeorm": "typeorm-ts-node-esm",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -d ./src/typeOrm.config.ts ./src/migration/$npm_config_name",
    "typeorm:migration:create": "npm run typeorm -- migration:create ./src/migration/$npm_config_name",
    "typeorm:migration:run": "npm run typeorm -- migration:run -d ./src/typeOrm.config.ts"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "dependencies": {
    "@fastify/static": "^6.6.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@types/eventsource": "^1.1.10",
    "axios": "^1.2.1",
    "chalk": "^4.1.0",
    "class-transformer": "^0.5.1",
    "eventsource": "^2.0.2",
    "fastify": "^3.29.4",
    "fastify-swagger": "^4.4.2",
    "fs-extra": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "socket.io-client": "^4.5.4",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "homebridge": "^1.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^8.0.17",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
